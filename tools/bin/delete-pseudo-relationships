#!/bin/bash
set -euo pipefail 

[ -f .env ] && source .env
J1_DEV_ENABLED=true yarn j1 -a j1dev -k "$J1_API_TOKEN" -q "Find (Function|Task) 
  that relates to as rx * where rx.pseudoRelationship = true return 
  rx._type as relationshipType,
  rx._class as relationshipClass,
  rx._fromEntityId as fromEntityId,
  rx._toEntityId as toEntityId,
  rx._key as relationshipKey"

J1_DEV_ENABLED=true yarn j1 -a j1dev -k "$J1_API_TOKEN" -o delete --relationship --hard-delete -f ./results.json 